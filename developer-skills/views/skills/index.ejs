<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Developer Skills</title>
</head>
<body>
    <h1>Skills</h1>
    <h1>Skills as of <%= time %></h1>
  
    <ul> 
     <% skills.forEach(function(t, idx) { %>
        <li>
          <a href="/skills/<%= idx %>"><%= t.skill %></a> 
          
        </li>
      <% }); %>
    </ul> 
    <a href="/skills/new">Add Skill</a>
</body>
</html>